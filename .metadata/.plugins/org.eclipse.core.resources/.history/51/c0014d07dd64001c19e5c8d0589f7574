
public class main {

	public static void main(String[] args) {
		String cantante= "Marcos";
		String hobby= "canta";
		System.out.println(cantante + " le gusta mucho el baile y mas cuando "+ hobby);
		
		int[] a =  {1,2,3,4};
		int[] b =  a;
		a = new int[2];
		System.out.println("b[0]: "+ a[0]);
		System.out.println("b[1]: "+ a[1]);
		System.out.println("b[2]: "+ a[2]);
	}

}
